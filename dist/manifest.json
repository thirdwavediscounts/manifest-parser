{
  "manifest_version": 3,
  "key": "",
  "name": "Manifest Parser",
  "description": "Parse manifest files from liquidation retail websites into unified CSV format",
  "version": "1.0.0",
  "permissions": [
    "activeTab",
    "downloads",
    "storage",
    "identity",
    "tabs",
    "scripting",
    "proxy",
    "webRequest",
    "webRequestAuthProvider"
  ],
  "host_permissions": [
    "*://*.bstock.com/*",
    "*://*.bstockauctions.com/*",
    "*://*.techliquidators.com/*",
    "*://*.amazon.com/*",
    "*://*.s3.amazonaws.com/*",
    "https://sheets.googleapis.com/*",
    "https://www.googleapis.com/*"
  ],
  "oauth2": {
    "client_id": "663454025753-hv2icekthq6uuc2cf8i6glt5el5mac1v.apps.googleusercontent.com",
    "scopes": [
      "https://www.googleapis.com/auth/spreadsheets.readonly"
    ]
  },
  "action": {
    "default_popup": "src/popup/index.html"
  },
  "background": {
    "service_worker": "service-worker-loader.js",
    "type": "module"
  },
  "content_scripts": [
    {
      "js": [
        "assets/bstock.ts-loader-XrbHlRvG.js"
      ],
      "matches": [
        "*://*.bstock.com/*",
        "*://*.bstockauctions.com/*"
      ]
    },
    {
      "js": [
        "assets/techliquidators.ts-loader-DOS1FxPc.js"
      ],
      "matches": [
        "*://*.techliquidators.com/*"
      ]
    },
    {
      "js": [
        "assets/amazon.ts-loader-CakEYEp8.js"
      ],
      "matches": [
        "*://*.amazon.com/*"
      ]
    }
  ],
  "web_accessible_resources": [
    {
      "matches": [
        "*://*.bstock.com/*",
        "*://*.bstockauctions.com/*"
      ],
      "resources": [
        "assets/detector-Bh9ZqayP.js",
        "assets/bstock.ts-UnHueT6N.js"
      ],
      "use_dynamic_url": false
    },
    {
      "matches": [
        "*://*.techliquidators.com/*"
      ],
      "resources": [
        "assets/detector-Bh9ZqayP.js",
        "assets/techliquidators.ts-DK2rgFmW.js"
      ],
      "use_dynamic_url": false
    },
    {
      "matches": [
        "*://*.amazon.com/*"
      ],
      "resources": [
        "assets/detector-Bh9ZqayP.js",
        "assets/amazon.ts-BrRjC2fq.js"
      ],
      "use_dynamic_url": false
    }
  ]
}
