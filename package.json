{
  "name": "manifest-parser",
  "version": "1.0.0",
  "description": "Chrome extension to parse manifest files from liquidation retail websites",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:e2e": "vitest run tests/e2e --testTimeout=30000",
    "test:e2e:pw": "npx playwright test"
  },
  "dependencies": {
    "jszip": "^3.10.1",
    "papaparse": "^5.4.1",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@crxjs/vite-plugin": "^2.0.0-beta.23",
    "@playwright/test": "^1.58.0",
    "@types/chrome": "^0.0.260",
    "@types/jsdom": "^27.0.0",
    "@types/papaparse": "^5.3.14",
    "csv-parse": "^6.1.0",
    "jsdom": "^27.4.0",
    "typescript": "^5.3.3",
    "vite": "^5.0.12",
    "vitest": "^1.2.2"
  }
}
